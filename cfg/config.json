{
   "auth":"system",
   "banner":"LinkLayerVPN Manager Script by @voltsshx",
   "limit_conn_single":-1,
   "limit_conn_request":-1,
   "services":[
      {
         "type":"httpdual",
         "cfg":{
            "Listen":"0.0.0.0:8000"
         }
      },
      {
         "type":"httpdual",
         "cfg":{
            "Key":"/etc/lnklyr/cfg/key.pem",
            "Cert":"/etc/lnklyr/cfg/cert.pem",
            "Limit":"1MB",
            "PacketBuffer":500,
            "IsTLS":true,
            "Listen":"0.0.0.0:8005"
         }
      },
      {
         "type":"tls",
         "cfg":{
            "Cert":"/etc/lnklyr/cfg/cert.pem",
            "Key":"/etc/lnklyr/cfg/key.pem",
            "Listen":"0.0.0.0:8001"
         }
      },
      {
         "type":"http",
         "cfg":{
            "Response":"HTTP/1.1 206 OK\r\n\r\n",
            "Listen":"0.0.0.0:8002"
         }
      },
      {
         "type":"http",
         "cfg":{
            "Response":"HTTP/1.1 200 OK\r\n\r\n",
            "Listen":"0.0.0.0:80"
         }
      },
      {
         "type":"httptls",
         "cfg":{
            "Http":{
               "Response":"HTTP/1.1 206 OK\r\n\r\n"
            },
            "TLS":{
               "Cert":"/etc/lnklyr/cfg/cert.pem",
               "Key":"/etc/lnklyr/cfg/key.pem"
            },
            "Listen":"0.0.0.0:8990"
         }
      },
      {
         "type":"httptls",
         "cfg":{
            "Http":{
               "Response":"HTTP/1.1 200 OK\r\n\r\n"
            },
            "TLS":{
               "Cert":"/etc/lnklyr/cfg/cert.pem",
               "Key":"/etc/lnklyr/cfg/key.pem"
            },
            "Listen":"0.0.0.0:443"
         }
      },
      {
         "type":"udp",
         "cfg":{
            "listen":":36718",
            "exclude":"53,5300",
            "net":"$netty",
            "cert":"/etc/lnklyr/layers/cfgs/lnklyr.crt",
            "key":"/etc/lnklyr/layers/cfgs/lnklyr.key",
            "obfs":"LnkLyrVPN2k24",
            "max_conn_client":500000
         }
      },
      {
         "type":"dnstt",
         "cfg":{
            "Domain":"$domain",
            "Net":"$netty"
         }
      }
   ]
}
